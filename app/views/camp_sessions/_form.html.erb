<%= form_for @camp_session do |f| %>
<%= render partial: 'common/errors', locals: {record: @camp_session} %>
<%= f.label :start_date, 'Start Date' %>
<%= f.date_select :start_date, {discard_year: true}, {} %>
<%= f.label :end_date, 'End Date' %>
<%= f.date_select :end_date, {discard_year: true}, {} %>
<%= f.label :camp_season, 'Camp Season' %>
<%= f.collection_select :camp_season_id, CampSeason.all, :id, :year, selected: CampSeason.most_recent.id %>
<%= f.submit %>
<% end %>
