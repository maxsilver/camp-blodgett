<button>Edit</button>
<button>Save</button>
<% @cabins.each do |cabin| %>
<div id="cabin_<%= cabin.id %>" class="cabin">
  <h1>Cabin <%= cabin.name %></h1>
  <table>
    <% cabin.kids.each do |kid| %>
    <tr id="kid_<%= kid.id %>" class="kid">
      <td class="name"><%= kid.name %></td>
      <td class="sex"><%= kid.sex %></td>
      <td class="age"><%= kid.age %></td>
      <td class="grade"><%= kid.grade %></td>
      <td class="move"></td>
    </tr>
    <% end %>
  </table>
</div>
<% end %>
